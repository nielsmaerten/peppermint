{
  "name": "peppermint",
  "version": "1.0.0-beta",
  "description": "Earth's best wallpapers: in your Dropbox",
  "keywords": [
    "wallpapers",
    "dropbox",
    "reddit"
  ],
  "private": true,
  "main": "dist/peppermint.umd.js",
  "module": "dist/peppermint.es5.js",
  "typings": "dist/types/peppermint.d.ts",
  "files": [
    "dist"
  ],
  "author": "Niels Maerten <niels@maerten.be>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nielsmaerten/peppermint"
  },
  "engines": {
    "node": "8"
  },
  "license": "MIT",
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "postinstall": "npm run build",
    "build": "tsc --project tsconfig.build.json && rollup -c && rimraf compiled",
    "test": "jest",
    "test:debug": "nodemon --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache --testNamePattern=ALL_TESTS_DISABLED",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg"
  },
  "devDependencies": {},
  "dependencies": {
    "@types/chai": "^4.0.3",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "@types/q": "^1.0.3",
    "@types/request": "^2.0.2",
    "@types/sinon": "^4.0.0",
    "chai": "^4.1.1",
    "colors": "^1.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "cuid": "^2.0.2",
    "dropbox": "^4.0.3",
    "es6-promise": "^4.2.4",
    "file-type": "^7.6.0",
    "firebase": "^4.3.0",
    "firebase-admin": "^5.13.1",
    "firebase-functions": "^2.0.4",
    "firebase-mock": "^2.0.14",
    "inversify": "^4.3.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.0.1",
    "lodash.camelcase": "^4.3.0",
    "moment": "^2.19.4",
    "nodemon": "^1.11.0",
    "prettier": "^1.4.4",
    "prompt": "^1.0.0",
    "q": "^1.5.0",
    "reflect-metadata": "^0.1.10",
    "replace-in-file": "^3.0.0",
    "request-image-size": "^2.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "sinon": "^4.5.0",
    "ts-jest": "^22.0.0",
    "ts-node": "^5.0.1",
    "tsc-watch": "^1.0.5",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.4.2"
  }
}
